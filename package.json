{
  "name": "newremixproject",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "remix vite:dev --host 0.0.0.0",
    "build": "remix vite:build",
    "build:tsc": "remix vite:build && tsc --project ./tsconfig.server.json",
    "dev:alt": "remix dev -c \"node ./server.js\"",
    "preview": "remix-serve ./build/server/index.js",
    "test": "vitest --browser=chromium",
    "coverage": "vitest run --coverage",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "npx prettier --write ./{app,test}/**/*.{ts,tsx}",
    "typecheck": "tsc",
    "start": "cross-env NODE_ENV=production node --enable-source-maps ./server/index.js",
    "fastly:preview": "remix-serve ./build/server/index.js",
    "fastly:prebuild": "tsc",
    "fastly:build": "js-compute-runtime build/server/index.js bin/main.wasm",
    "fastly:deploy": "fastly compute publish",
    "build:fastly": "npm run build:remix && npm run build:fastly",
    "build:remix": "remix build",
    "prebuild:fastly": "compute-js-static-publish --build-static --suppress-framework-warnings",
    "test:browser": "vitest --workspace=vitest.workspace.ts"
  },
  "dependencies": {
    "@fastly/js-compute": "^3.21.4",
    "@fastly/remix-server-adapter": "^4.0.0",
    "@fastly/remix-server-runtime": "^4.0.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@remix-run/css-bundle": "^2.11.2",
    "@remix-run/express": "^2.11.2",
    "@remix-run/node": "^2.11.2",
    "@remix-run/react": "^2.11.2",
    "@remix-run/serve": "^2.11.2",
    "@remix-run/testing": "^2.11.2",
    "aws-amplify": "^6.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "init": "^0.1.2",
    "isbot": "^5.1.17",
    "lucide-react": "^0.439.0",
    "shadcn-ui": "^0.9.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.2.1",
    "@aws-amplify/backend-cli": "^1.2.5",
    "@biomejs/biome": "1.8.3",
    "@remix-run/dev": "^2.11.2",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^2.0.5",
    "@vitest/coverage-istanbul": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "aws-cdk": "^2.156.0",
    "aws-cdk-lib": "^2.156.0",
    "constructs": "^10.3.0",
    "esbuild": "^0.23.1",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.47.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
