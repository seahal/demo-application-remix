{
  "name": "newremixproject",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "remix vite:dev",
    "build": "remix vite:build",
    "build:tsc": "remix vite:build && tsc --project ./tsconfig.server.json",
    "dev:alt": "remix dev -c \"node ./server.js\"",
    "preview": "remix-serve ./build/server/index.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "start": "cross-env NODE_ENV=production node --enable-source-maps ./server/index.js",
    "fastly:preview": "remix-serve ./build/server/index.js",
    "fastly:prebuild": "tsc",
    "fastly:build": "js-compute-runtime build/server/index.js bin/main.wasm",
    "fastly:deploy": "fastly compute publish",
    "build:fastly": "npm run build:remix && npm run build:fastly",
    "build:remix": "remix build",
    "prebuild:fastly": "compute-js-static-publish --build-static --suppress-framework-warnings"
  },
  "dependencies": {
    "@fastly/remix-server-adapter": "^4.0.0",
    "@fastly/remix-server-runtime": "^4.0.0",
    "@remix-run/css-bundle": "^2.11.1",
    "@remix-run/react": "^2.11.1",
    "@remix-run/serve": "^2.11.1",
    "@remix-run/testing": "^2.11.1",
    "esbuild": "^0.17.6",
    "cross-env": "^7.0.3",
    "@remix-run/node": "^2.11.1",
    "@fastify/middie": "^8.3.1",
    "@fastify/static": "^7.0.4",
    "@fastly/js-compute": "^3.19.0",
    "@mcansh/remix-fastify": "3.3.3",
    "fastify": "^4.28.1",
    "isbot": "^5.1.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.10.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.8.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.3.5",
    "vite-tsconfig-paths": "^4.2.1",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
